  "components": {
    "securitySchemes": {
      "JWT": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "in": "header",
        "name": "Authorization",
        "description": "Paste: Bearer <your-JWT>"
      }
    },
    "schemas": {
      "PostBookingRequestDto": {
        "type": "object",
        "properties": {
          "restarauntId": {
            "type": "string",
            "description": "Restaurant identifier (UUID string).",
            "format": "uuid",
            "example": "2f7a9e0b-2c10-4f5e-8a2c-3b5d8f5e9a11"
          },
          "bookAt": {
            "type": "string",
            "description": "ISO-8601 timestamp at whole-hour granularity (e.g., 18:00, 19:00...). Must be in the future.",
            "format": "date-time",
            "example": "2025-11-04T18:00:00.000Z"
          },
          "amountOfGuests": {
            "type": "number",
            "description": "Number of guests (integer ≥ 1).",
            "example": 2,
            "minimum": 1
          }
        },
        "required": [
          "restarauntId",
          "bookAt",
          "amountOfGuests"
        ]
      },
      "PostBookingResponseDto": {
        "type": "object",
        "properties": {
          "bookingRequestId": {
            "type": "string",
            "description": "Booking Request identifier (UUID string).",
            "format": "uuid",
            "example": "9f2a3b7e-7d3a-4c15-9e4c-4f1a2b3c4d5e"
          }
        },
        "required": [
          "bookingRequestId"
        ]
      },
      "GetBookingResponseDto": {
        "type": "object",
        "properties": {
          "bookingRequestId": {
            "type": "string",
            "description": "Public booking identifier (UUID string). In this model it equals the bookingRequestId.",
            "format": "uuid",
            "example": "9f2a3b7e-7d3a-4c15-9e4c-4f1a2b3c4d5e"
          },
          "restarauntId": {
            "type": "string",
            "description": "Restaurant identifier (UUID string).",
            "format": "uuid",
            "example": "2f7a9e0b-2c10-4f5e-8a2c-3b5d8f5e9a11"
          },
          "bookAt": {
            "type": "string",
            "description": "ISO-8601 timestamp at whole-hour granularity (e.g., 18:00, 19:00...).",
            "format": "date-time",
            "example": "2025-11-04T18:00:00.000Z"
          },
          "amountOfGuests": {
            "type": "number",
            "description": "Number of guests (integer ≥ 1).",
            "example": 2,
            "minimum": 1
          },
          "status": {
            "type": "string",
            "description": "Status string. Allowed values (documented only): \"CREATED\", \"CONFIRMED\", \"REJECTED\".",
            "example": "CREATED"
          }
        },
        "required": [
          "bookingRequestId",
          "restarauntId",
          "bookAt",
          "amountOfGuests",
          "status"
        ]
      }
    }
